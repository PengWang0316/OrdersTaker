{
  "name": "orderstaker",
  "version": "0.0.1",
  "description": "A progressive web app that aims to help restaurants taking order easier.",
  "main": "index.js",
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --mode development --open",
    "build": "set NODE_ENV=production & webpack --mode production -p",
    "deploy": "scp -i c:/projects/orderstaker.pem c:/projects/orderstaker/dist/*.* ubuntu@orderstaker.kevin-project.com:/var/www/html/",
    "test": "jest",
    "testCoverage": "jest --coverage --runInBand",
    "testWatch": "jest --watch",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "keywords": [
    "order",
    "taker",
    "react",
    "pwa"
  ],
  "author": "Kevin W.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PengWang0316/OrdersTaker.git"
  },
  "sideEffects": false,
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", "/__tests__/e2e/"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/tools/assetsTransformer.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/app/tools/setupTests.js"
    ],
    "coveragePathIgnorePatterns": [
      "app/resources/",
      "app/tools",
      "index.js"
    ],
    "testEnvironment": "jest-environment-jsdom-global",
    "testURL": "http://localhost",
    "verbose": true
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "dependencies": {
    "@kevinwang0316/react-materialui-pagination": "^1.1.3",
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "animejs": "^2.2.0",
    "axios": "^0.18.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-imported-component": "^4.6.2",
    "react-live-clock": "^2.0.3",
    "react-qr-reader": "^2.1.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^2.1.1",
    "workbox-sw": "^3.4.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.2",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "puppeteer": "^1.8.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.4.2",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.4",
    "style-loader": "^0.21.0",
    "uglify-es": "^3.3.10",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.4",
    "webpack-bundle-analyzer": "^2.12.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "workbox-webpack-plugin": "^3.4.1"
  }
}
