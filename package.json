{
  "name": "orderstaker",
  "version": "0.0.1",
  "description": "A progressive web app that aims to help restaurants taking order easier.",
  "main": "index.js",
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --mode development --open",
    "build": "set NODE_ENV=production & webpack --mode production -p",
    "test": "jest",
    "testCoverage": "jest --coverage --runInBand",
    "testWatch": "jest --watch",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "keywords": [
    "order",
    "taker",
    "react",
    "pwa"
  ],
  "author": "Kevin W.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PengWang0316/OrdersTaker.git"
  },
  "sideEffects": false,
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/tools/assetsTransformer.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-localstorage-mock",
      "raf/polyfill",
      "<rootDir>/app/tools/setupTests.js"
    ],
    "coveragePathIgnorePatterns": [
      "app/resources/",
      "app/tools",
      "index.js"
    ],
    "testEnvironment": "jest-environment-jsdom-global"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "@material-ui/icons": "^1.1.0",
    "animejs": "^2.2.0",
    "axios": "^0.18.0",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.3",
    "react-imported-component": "^4.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.7.9",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "workbox-sw": "^3.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "jest-environment-jsdom": "^23.2.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1",
    "sinon": "^5.0.7",
    "style-loader": "^0.21.0",
    "uglify-es": "^3.3.10",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.12.0",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4",
    "workbox-webpack-plugin": "^3.2.0"
  }
}
