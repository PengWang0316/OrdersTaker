{
  "name": "orderstaker",
  "version": "0.0.1",
  "description": "A progressive web app that aims to help restaurants taking order easier.",
  "main": "index.js",
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --mode development --open",
    "build": "set NODE_ENV=production & webpack --mode=production -p",
    "deploy": "scp -i c:/projects/orderstaker.pem c:/projects/orderstaker/dist/*.* ubuntu@orderstaker.kevin-project.com:/var/www/html/",
    "test": "jest",
    "testCoverage": "jest --coverage --runInBand",
    "testWatch": "jest --watch",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "keywords": [
    "order",
    "taker",
    "react",
    "pwa"
  ],
  "author": "Kevin W.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PengWang0316/OrdersTaker.git"
  },
  "sideEffects": false,
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "e2e/*"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/tools/assetsTransformer.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/app/tools/setupTests.js"
    ],
    "coveragePathIgnorePatterns": [
      "app/resources/",
      "app/tools",
      "index.js"
    ],
    "testEnvironment": "jest-environment-jsdom-global",
    "testURL": "http://localhost",
    "verbose": true
  },
  "dependencies": {
    "@kevinwang0316/react-materialui-pagination": "^1.1.3",
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "animejs": "^2.2.0",
    "axios": "^0.19.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-imported-component": "^5.2.1",
    "react-live-clock": "^3.0.0",
    "react-qr-reader": "^2.1.1",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^2.1.1",
    "workbox-sw": "^3.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-environment-jsdom": "^24.8.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "puppeteer": "^1.17.0",
    "raf": "^3.4.1",
    "react-test-renderer": "^16.6.3",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.1.1",
    "style-loader": "^0.23.1",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.4.1",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
